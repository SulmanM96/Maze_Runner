<Window x:Class="GeometryDash.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeometryDash"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1000">
    <Canvas Width="1000" Margin="0,0,-6.4,-1.6">
        <Rectangle x:Name="Player" Fill="#FFF4F4F5" Height="22" Canvas.Left="133" Stroke="Black" Canvas.Top="478" Width="24" Tag="Player"/>
        <Rectangle x:Name="LeftBar" Fill="#FF434343" Height="361" Canvas.Left="0" Stroke="Black" Canvas.Top="411" Width="25" Tag="LeftBar"/>
        <Rectangle x:Name="RightBar" Fill="#FF434343" Height="375" Canvas.Left="973" Stroke="Black" Canvas.Top="0" Width="25" Tag="RightBar"/>
        <Rectangle x:Name="BottomBar" Fill="#FF434343" Height="25" Canvas.Left="0" Stroke="Black" Canvas.Top="747" Width="447" Tag="BottomBar1"/>
        <Rectangle x:Name="TopBar" Fill="#FF434343" Height="25" Canvas.Left="0" Stroke="Black" Canvas.Top="0" Width="447" Tag="TopBar"/>
        <Button Content="Easy" Height="62" Canvas.Left="394" Canvas.Top="288" Width="182" Click="Button_Click"/>
        <Button Content="Medium" Height="62" Canvas.Left="394" Canvas.Top="401" Width="182" Click="Button_Click2"/>
        <Button Content="Hard" Height="62" Canvas.Left="394" Canvas.Top="511" Width="182" Click="Button_Click3"/>
        <Label Content="Maze Runner" Height="127" Canvas.Left="236" Canvas.Top="77" Width="530" FontFamily="Showcard Gothic" FontSize="72"/>
        <Rectangle x:Name="BottomBar2" Fill="#FF434343" Height="25" Canvas.Left="479" Stroke="Black" Canvas.Top="747" Width="519" Tag="BottomBar2"/>
        <Rectangle x:Name="TopBar2" Fill="#FF434343" Height="25" Canvas.Left="479" Stroke="Black" Canvas.Top="0" Width="519" Tag="TopBar2"/>
        <Rectangle x:Name="RightBar2" Fill="#FF434343" Height="361" Canvas.Left="973" Stroke="Black" Canvas.Top="411" Width="25" Tag="RightBar2"/>
        <Rectangle x:Name="LeftBar2" Fill="#FF434343" Height="375" Canvas.Left="0" Stroke="Black" Canvas.Top="0" Width="25" Tag="LeftBar2"/>
        <Rectangle x:Name="SecretLevel" Fill="#FF434343" Height="7" Canvas.Left="436" Stroke="Black" Canvas.Top="765" Width="55" Tag="SecretLevel" Opacity="0"/>

    </Canvas>
</Window>
